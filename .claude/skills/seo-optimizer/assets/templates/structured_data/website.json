{
  "@context": "https://schema.org",
  "@type": "WebSite",
  "name": "{website_name}",
  "alternateName": "{alternate_name}",
  "url": "{website_url}",
  "description": "{website_description}",
  "inLanguage": "en-US",
  "isAccessibleForFree": true,
  "isPartOf": {
    "@type": "WebSite",
    "name": "{website_name}",
    "url": "{website_url}"
  },
  "publisher": {
    "@type": "Organization",
    "name": "{author_name}",
    "url": "{author_url}",
    "logo": {
      "@type": "ImageObject",
      "url": "{logo_url}",
      "width": 600,
      "height": 60
    }
  },
  "author": {
    "@type": "Person",
    "name": "{author_name}",
    "url": "{author_url}",
    "sameAs": [
      "{author_social_links}"
    ]
  },
  "copyrightYear": "{copyright_year}",
  "copyrightHolder": {
    "@type": "Person",
    "name": "{author_name}"
  },
  "dateModified": "{last_updated}",
  "datePublished": "{launch_date}",
  "genre": "Professional Portfolio",
  "keywords": "{website_keywords}",
  "audience": {
    "@type": "Audience",
    "audienceType": "Recruiters, Hiring Managers, Technical Leads"
  },
  "mainEntity": {
    "@type": "Person",
    "name": "{author_name}",
    "jobTitle": "{author_title}",
    "knowsAbout": [
      "{author_skills}"
    ]
  },
  "about": {
    "@type": "Thing",
    "name": "Software Development Portfolio",
    "description": "Professional portfolio showcasing software development projects and skills"
  },
  "potentialAction": {
    "@type": "SearchAction",
    "target": "{website_url}/search?q={search_term_string}",
    "query-input": "required name=search_term_string"
  },
  "sameAs": [
    "{github_url}",
    "{linkedin_url}",
    "{twitter_url}",
    "{portfolio_url}"
  ]
}